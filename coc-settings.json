{
    "rust-analyzer.serverPath":"/Volumes/Transcend/GitHub/rust-analyzer/target/release/rust-analyzer",
    "languageserver": {
        "rust": {
          "command": "rust-analyzer",
          "filetypes": ["rust"],
          "rootPatterns": ["Cargo.toml"]
        },
        "golang": {
          "command": "gopls",
          "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
          "filetypes": ["go"],
          "initializationOptions": {
            "usePlaceholders": true
          }
        }
    }
}
