{
    "diagnostic.displayByAle": true,
    "languageserver": {
        "rust": {
          "command": "ra_lsp_server",
          "filetypes": ["rust"],
          "rootPatterns": ["Cargo.toml"]
        },
        "golang": {
          "command": "gopls",
          "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
          "filetypes": ["go"],
          "initializationOptions": {
            "usePlaceholders": true
          }
        }
    },
    "list.source.grep.command": "Ag"
}
